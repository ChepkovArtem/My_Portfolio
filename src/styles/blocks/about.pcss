.about{
    background-color: greenyellow;
}
.about__container{
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 10% 0;
}
.about__left{
    width: 30%;
}
.about__right{
    width: 60%;
    display: flex;
    flex-direction: column;
    justify-content: center;
}
td{
    padding: 20px 20px 0;
    color: $subtext;
    &:not(:first-child){
    color: $text;
}
}